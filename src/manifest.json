{
  "manifest_version": 2,
  "name": "A-Zoom Manager",
  "homepage_url": "http://localhost:8080/",
  "description": "Useful extension to manage zoom meetings",
  "default_locale": "en",
  "version": 1.0,
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "A-Zoom Manager",
    "default_icon": {
      "19": "icons/19.png",
      "48": "icons/48.png"
    }
  }
}
